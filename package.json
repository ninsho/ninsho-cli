{
  "name": "ninsho-cli",
  "version": "0.0.6",
  "description": "A command line tool useful for creating database tables and other tasks on ninsho.",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "module": "dist/index.esm.js",
  "browser": "dist/ninshoCli.js",
  "bin": {
    "ninsho-cli": "dist/bin/index.js"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "types",
    "dist"
  ],
  "keywords": [
    "ninsho"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c && mkdir -p dist/bin/src && cp -R src/schema dist/bin/src",
    "lint": "eslint src/**/*.ts",
    "format": "eslint --fix src/**/*.ts",
    "lint-staged": "lint-staged",
    "start": "browser-sync start --config \"./.bs-config.js\"",
    "test": "jest",
    "dev": "npx ts-node-dev --respawn src/index.ts",
    "command": "npx ts-node-dev --respawn src/bin/index.ts"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": "https://github.com/ninsho/ninsho-cli/issues"
  },
  "homepage": "https://github.com/ninsho/ninsho-cli/",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@jest/types": "^29.3.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^10.5.4",
    "lodash.camelcase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.3"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "minimist": "^1.2.8",
    "pg": "^8.10.0"
  }
}
